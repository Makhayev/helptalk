{
  "env": {
    "es2020": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parser":"@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "requireConfigFile": false,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "error"
  }
}